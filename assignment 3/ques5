#include <iostream>
using namespace std;

int main() {
    char exp[100];
    int st[100], top = -1;
    cout << "Enter Postfix: ";
    cin >> exp;

    for (int i = 0; exp[i] != '\0'; i++) {
        char c = exp[i];
        if (c >= '0' && c <= '9') {
            st[++top] = c - '0'; 
        }  
        else {
            int b = st[top--];
            int a = st[top--];
            switch (c) {
                case '+': st[++top] = a + b; break;
                case '-': st[++top] = a - b; break;
                case '*': st[++top] = a * b; break;
                case '/': st[++top] = a / b; break;
            }
        }
    }
    cout << "Result = " << st[top];
}
